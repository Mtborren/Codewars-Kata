8

  int findDifference(List<int> a, List<int> b) {
    var totalA = 1;
    var totalB = 1;
    for (var x in a) {
      totalA *= x;
    }
    for (var x in b) {
      totalB *= x;
    }
    if (totalA > totalB) {
      return totalA - totalB;
    } else {
      return totalB - totalA;
    }
  }
