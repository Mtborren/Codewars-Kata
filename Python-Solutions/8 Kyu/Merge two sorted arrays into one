8

    def merge_arrays(arr1, arr2):
        new_arr = []
        for x in arr1:
            if x not in new_arr:
                new_arr.append(x)
        for x in arr2:
            if x not in new_arr:
                new_arr.append(x)
        return sorted(new_arr)
