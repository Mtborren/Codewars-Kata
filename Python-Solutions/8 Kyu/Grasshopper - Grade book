https://www.codewars.com/kata/55cbd4ba903825f7970000f5/train/python

    def get_grade(s1, s2, s3):
        sum = (s1 + s2 + s3) / 3
        match sum:
            case sum if 90 <= sum <= 100:
                return "A"
            case sum if 80 <= sum < 90:
                return "B"
            case sum if 70 <= sum < 80:
                return "C"
            case sum if 60 <= sum < 70:
                return "D"
            case sum if 0 <= sum < 60:
                return "F"
