    def find_longest(arr):
        lrgnum = []
        for x in arr:
            print(x)
            if lrgnum == []:
                lrgnum.append(str(x))
            elif len(str(x)) > len(lrgnum[0]):
                lrgnum[0] = str(x)
        return int(lrgnum[0])
