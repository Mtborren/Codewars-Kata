7

    def longest(a1, a2):
        newarr = []
        for let1 in a1:
            for let2 in a2:
                if let1 not in newarr:
                    newarr.append(let1)
                elif let2 not in newarr:
                    newarr.append(let2)
        return "".join(sorted(newarr))
