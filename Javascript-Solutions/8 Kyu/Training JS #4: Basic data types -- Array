  getLength = (arr) => arr.length
  getFirst = (arr) => arr[0]
  getLast = (arr) => arr[arr.length-1]

  pushElement = (arr) => {
    var el=1;
    arr.push(el)
    return arr
  }

  popElement = (arr) => {
    arr.pop()
    return arr
  }
